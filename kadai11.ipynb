      "source": [
        "array = [\"水\",\"金\",\"地\",\"火\",\"木\",\"土\",\"天\",\"海\",\"冥\"]\n",
        "\n",
        "#for文の場合\n",
        "for value in array:\n",
        "     print(f\"{value}\")\n",
        "\n",
        "#while文の場合\n",
        "#カウンタ変数の用意\n",
        "i = 0\n",
        "while i < len(array):\n",
        "    print(array[i])\n",
        "    #インデックスを1つ進める\n",
        "    i += 1\n",
        "\n",
        "    if i == len(array):\n",
        "     break\n"
      ]
